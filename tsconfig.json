{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "jsxFactory": "h",
    "allowJs": true,
    "skipLibCheck": true,
  }
}